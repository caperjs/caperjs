<script src="/app/templates/navbar/navbar.js"></script>
<link href="/app/styles/min/navbar.css" rel="stylesheet">
<nav data-template="navbar" class="navbar navbar-default container-fluid margin-none">
    <script data-script="navbar" type="text/x-handlebars-template">
    <header class="navbar-header">
        <a class="navbar-brand" href="{{headerURL}}">{{websiteName}}</a>
    </header>
    <ul class="nav navbar-nav">
        {{#each this.pages}}
        <li {{#if @first}}class="active"{{/if}} data-navigation="{{@key}}"><a href="#" onmouseup="changePage('{{@key}}');">{{@key}}</a></li>
        {{/each}}
    </ul>
    </script>
</nav>